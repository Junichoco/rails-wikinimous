<div class="container">
  <div class="col-6 card">
  <h1>New Article</h1>
  <%= form_with model: @article do |f| %>
    <p><%= f.label :title %></p>
    <p><%= f.text_field :title %></p>
    <p><%= f.label :content %></p>
    <p><%= f.text_field :content %></p>
    <p><%= f.submit "Add task", class: "btn btn-success mt-3" %></p>
  <% end %>

  <%= link_to "Go back", articles_path %>
  </div>
</div>
